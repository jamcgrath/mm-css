{
  ".flex-row": {
    "flexDirection": "row"
  },
  ".flex-reverse": {
    "flexDirection": "row-reverse"
  },
  ".flex-col": {
    "flexDirection": "column"
  },
  ".flex-col-reverse": {
    "flexDirection": "column-reverse"
  },
  "@media (min-width: 1024px)": {
    ".lg-flex-row": {
      "flexDirection": "row"
    },
    ".lg-flex-reverse": {
      "flexDirection": "row-reverse"
    },
    ".lg-flex-col": {
      "flexDirection": "column"
    },
    ".lg-flex-col-reverse": {
      "flexDirection": "column-reverse"
    }
  }
}