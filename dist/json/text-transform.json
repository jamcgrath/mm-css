{
  ".text-transform-none": {
    "--text-transform": "none",
    "textTransform": "var(--text-transform)"
  },
  ".capitalize": {
    "--text-transform": "capitalize",
    "textTransform": "var(--text-transform)"
  },
  ".uppercase": {
    "--text-transform": "uppercase",
    "textTransform": "var(--text-transform)"
  },
  ".lowercase": {
    "--text-transform": "lowercase",
    "textTransform": "var(--text-transform)"
  }
}