{
  "breadcrumb": {
    "fontSize": "var(--body)",
    "fontWeight": "var(--regular)",
    "listStyle": "1.5",
    "fontFamily": "var(--poppins)",
    "color": "var(--breadcrumb-color, var(--black))",
    "backgroundColor": "var(--breadcrumb-bg, transparent)",
    "padding": "var(--space-1) var(--space-3)",
    "display": "inline-flex",
    "justifyContent": "center",
    "alignItems": "center",
    "borderRadius": "8px",
    "textDecoration": "none"
  },
  "breadcrumb:hover": {
    "--breadcrumb-color": "var(--grape-hover)"
  },
  "bread-crumb:focus,\nbread-crumb:active,\nbread-crumbcurrent": {
    "--breadcrumb-color": "var(--white)",
    "--breadcrumb-bg": "var(--grape)"
  },
  "breadcrumbdisabled,\nbreadcrumb[aria-disabled=\"true\"],\nbreadcrumbdisabled:hover,\nbreadcrumb[aria-disabled=\"true\"]:hover": {
    "--breadcrumb-color": "var(--gray-5)",
    "--breadcrumb-bg": "var(--gray-3)",
    "pointerEvents": "none"
  }
}