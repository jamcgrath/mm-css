{
  ".list-reset": {
    "listStyle": "none",
    "padding": "0",
    "margin": "0"
  },
  ".list-style-none": {
    "listStyle": "none"
  },
  ".list-style-square": {
    "listStyle": "square"
  },
  ".list-style-circle": {
    "listStyle": "circle"
  },
  ".list-style-disc": {
    "listStyle": "disc"
  },
  ".list-style-decimal": {
    "listStyle": "decimal"
  },
  ".list-in": {
    "listStylePosition": "inside"
  },
  ".list-out": {
    "listStylePosition": "outside"
  },
  ".list-check-sm,\n  .list-check-med,\n  .list-check-lg": {
    "--_list-fs": "var(--list-fs, inherit)",
    "listStyle": "none",
    "margin": "0",
    "padding": "0",
    "fontSize": "var(--_list-fs)",
    "fontWeight": 400,
    "lineHeight": "var(--lh-125)",
    "color": "var(--gray-6)",
    "width": "max-content"
  },
  ".list-check-sm": {
    "--list-fs": "var(--caption)"
  },
  ".list-check-med": {
    "--list-fs": "var(--body)"
  },
  ".list-check-lg": {
    "--list-fs": "var(--title-6)"
  },
  "ul[class*='list-check'] li": {
    "--list-gap": "calc(var(--list-fs) + var(--space-2))",
    "display": "grid",
    "gridTemplateColumns": "0 1fr",
    "alignItems": "center",
    "gap": "0 var(--list-gap)"
  },
  "ul[class*='list-check'] li:before": {
    "fontFamily": "mm-icons !important",
    "fontWeight": "normal !important",
    "fontSize": "inherit",
    "content": "'\\f22d'",
    "color": "var(--grape)",
    "fontStyle": "normal",
    "fontFeatureSettings": "normal",
    "fontVariant": "normal",
    "textTransform": "none",
    "lineHeight": 1,
    "WebkitFontSmoothing": "antialiased",
    "MozOsxFontSmoothing": "grayscale"
  },
  "ul[class*='list-check'] > li > ul[class*='list-check']": {
    "padding": "0 var(--list-gap)"
  },
  "ul[class*='list-check']:not([role='list'])": {
    "outline": "1px solid var(--error)"
  },
  "ul[class*='list-check']:not([role='list']):after": {
    "content": "'ul needs role=list'",
    "color": "var(--error)",
    "display": "block"
  },
  "@media (min-width: 1024px)": {
    ".lg-list-check-sm,\n.lg-list-check-med,\n.lg-list-check-lg": {
      "--_list-fs": "var(--list-fs, inherit)",
      "listStyle": "none",
      "margin": "0",
      "padding": "0",
      "fontSize": "var(--_list-fs)",
      "fontWeight": 400,
      "lineHeight": "var(--lh-125)",
      "color": "var(--gray-6)",
      "width": "max-content"
    },
    ".lg-list-check-sm": {
      "--list-fs": "var(--caption)"
    },
    ".lg-list-check-med": {
      "--list-fs": "var(--body)"
    },
    ".lg-list-check-lg": {
      "--list-fs": "var(--title-6)"
    }
  }
}