{
  ".grid-auto-col": {
    "gridAutoColumns": "auto"
  },
  ".grid-auto-col-min": {
    "gridAutoColumns": [
      "-webkit-min-content",
      "min-content"
    ]
  },
  ".grid-auto-col-max": {
    "gridAutoColumns": [
      "-webkit-max-content",
      "max-content"
    ]
  },
  "@media (min-width: 1024px)": {
    ".lg-grid-auto-col": {
      "gridAutoColumns": "auto"
    },
    ".lg-grid-auto-col-min": {
      "gridAutoColumns": [
        "-webkit-min-content",
        "min-content"
      ]
    },
    ".lg-grid-auto-col-max": {
      "gridAutoColumns": [
        "-webkit-max-content",
        "max-content"
      ]
    }
  }
}