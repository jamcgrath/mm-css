{
  ".container,\n.sm-container,\n.md-container,\n.lg-container,\n.xl-container,\n.xxl-container": {
    "width": "100%"
  },
  ".sm-container": {
    "maxWidth": "480px"
  },
  ".md-container": {
    "maxWidth": "768px"
  },
  ".lg-container": {
    "maxWidth": "1024px"
  },
  ".xl-container": {
    "maxWidth": "1280px"
  },
  ".xxl-container": {
    "maxWidth": "1920px"
  },
  ".fullwidth": {
    "width": "100vw",
    "position": "relative",
    "left": "50%",
    "right": "50%",
    "marginLeft": "-50vw",
    "marginRight": "-50vw"
  }
}