{
  "button,\n.btn": {
    "background": "var(--btn-bg, transparent)",
    "color": "var(--btn-color, currentcolor)",
    "border": "none",
    "fontSize": "var(--btn-fs, var(--label))",
    "fontWeight": "var(--btn-fw, var(--fw-400))",
    "lineHeight": "var(--btn-lh, var(--lh-125))",
    "padding": "var(--btn-paddin, 0)",
    "textAlign": "var(--btn-text-align, center)",
    "cursor": "var(--btn-cursor, pointer)"
  },
  ".btn": {
    "borderRadius": "var(--btn-border-radius, var(--space-2))",
    "display": "var(--btn-display, inline-flex)",
    "justifyContent": "var(--btn-justify, center)",
    "alignItems": "var(--btn-align, center)",
    "gap": "var(--btn-gap, 0)"
  },
  ".btn > *": {
    "pointerEvents": "none"
  },
  ".btn-primary": {
    "--btn-bg": "var(--navy)",
    "--btn-color": "var(--white)"
  },
  ".btn-primary:hover": {
    "--btn-bg": "var(--navy-hover)"
  },
  ".btn-primary:focus": {
    "--btn-bg": "var(--navy-pressed)"
  },
  ".btn-tonal": {
    "--btn-bg": "var(--navy-light)",
    "--btn-color": "var(--navy)"
  },
  ".btn-tonal:hover": {
    "--btn-bg": "var(--navy-light-hover)"
  },
  ".btn-tonal:focus": {
    "--btn-bg": "var(--navy-light-pressed)",
    "--btn-focus-color": "var(--navy)"
  },
  ".btn-outline,\n.btn-outline-secondary,\n.btn-outline-destructive": {
    "backgroundColor": "var(--btn-bg) !important",
    "--btn-bg": "transparent",
    "--btn-border-color": "currentcolor",
    "--btn-border-style": "solid",
    "--btn-border-width": "var(--space-1px)",
    "--btn-color": "var(--navy)",
    "border": "var(--btn-border-width) var(--btn-border-style)\n    var(--btn-border-color)"
  },
  ".btn-outline:hover": {
    "--btn-color": "var(--navy-hover)"
  },
  ".btn-outline:focus": {
    "--btn-color": "var(--navy-pressed)"
  },
  ".btn-flat,\n.btn-flat-secondary,\n.btn-flat-destructive": {
    "background": "transparent !important",
    "border": "0 !important",
    "--btn-color": "var(--navy)"
  },
  ".btn-flat:hover": {
    "--btn-color": "var(--navy-hover)"
  },
  ".btn-flat:focus": {
    "--btn-color": "var(--navy-pressed)"
  },
  ".btn-secondary": {
    "--btn-bg": "var(--grape)",
    "--btn-color": "var(--white)"
  },
  ".btn-secondary:hover": {
    "--btn-bg": "var(--grape-hover)",
    "--btn-color": "var(--white)"
  },
  ".btn-secondary:focus": {
    "--btn-bg": "var(--grape-pressed)",
    "--btn-color": "var(--white)"
  },
  ".btn-tonal-secondary": {
    "--btn-bg": "var(--grape-light)",
    "--btn-color": "var(--grape)"
  },
  ".btn-outline-secondary": {
    "--btn-color": "var(--grape)"
  },
  ".btn-outline-secondary:hover": {
    "--btn-color": "var(--grape-hover)"
  },
  ".btn-outline-secondary:focus": {
    "--btn-color": "var(--grape-pressed)"
  },
  ".btn-flat-secondary": {
    "--btn-color": "var(--grape)"
  },
  ".btn-flat-secondary:hover": {
    "--btn-color": "var(--grape-hover)"
  },
  ".btn-flat-secondary:focus": {
    "--btn-color": "var(--grape-pressed)"
  },
  ".btn-destructive": {
    "--btn-bg": "var(--error)",
    "--btn-color": "var(--white)"
  },
  ".btn-destructive:hover": {
    "--btn-bg": "var(--error-hover)"
  },
  ".btn-destructive:focus": {
    "--btn-bg": "var(--error-pressed)"
  },
  ".btn-outline-destructive,\n.btn-flat-destructive": {
    "--btn-color": "var(--error)"
  },
  ".btn-outline-destructive:hover,\n.btn-flat-destructive:hover": {
    "--btn-color": "var(--error-hover)"
  },
  ".btn-outline-destructive:focus,\n.btn-flat-destructive:focus": {
    "--btn-color": "var(--error-pressed)"
  },
  ".btn-sm": {
    "--btn-padding": "var(--space-1) var(--space-3)"
  },
  ".btn-med": {
    "--btn-padding": "var(--space-2) var(--space-4)"
  },
  ".btn-lg": {
    "--btn-padding": "var(--space-3) var(--space-6)"
  },
  ".btn[disabled]": {
    "--btn-bg": "var(--gray-3)",
    "--btn-color": "var(--gray-6)",
    "--btn-cursor": "not-allowed"
  },
  ".btn-flat[disabled],\n.btn-flat-secondary[disabled],\n.btn-flat-destructive[disabled]": {
    "background": "transparent !important"
  },
  ".btn-circle": {
    "borderRadius": "50% !important",
    "--btn-circle-width": " ",
    "aspectRatio": "1 / 1",
    "width": "var(--btn-circle-width)",
    "height": "var(--btn-circle-width)",
    "overflow": "hidden"
  },
  ".btn-circle.btn-sm": {
    "--btn-padding": "6px",
    "--btn-circle-width": "24px"
  },
  ".btn-circle.btn-med": {
    "--btn-padding": "var(--spacing-2)",
    "--btn-fs": "var(--body)",
    "--btn-circle-width": "40px"
  },
  ".btn-circle.btn-lg": {
    "--btn-padding": "var(--spacing-4)",
    "--btn-fs": "var(--body)",
    "--btn-circle-width": "56px"
  },
  ".btn .mmi": {
    "--btn-mmi-fs": "var(--btn-fs)",
    "fontSize": "var(--btn-mmi-fs)"
  },
  ".btn-icon.btn-sm,\n.btn-icon.btn-med": {
    "--gap": "var(--space-1)"
  },
  ".btn-icon.btn-lg": {
    "--gap": "var(--space-2)"
  },
  ".btn.focus-visible": {
    "--btn-focus-color": "var(--navy-hover)",
    "outline": "2px solid var(--btn-focus-color)",
    "outlineOffset": "2px"
  },
  ".btn:focus-visible": {
    "--btn-focus-color": "var(--navy-hover)",
    "outline": "2px solid var(--btn-focus-color)",
    "outlineOffset": "2px"
  },
  ".btn-secondary:focus,\n.btn-tonal-secondary:focus,\n.btn-flat-secondary:focus,\n.btn-outline-secondary:focus": {
    "--btn-focus-color": "var(--grape-pressed)"
  },
  ".btn-destructive:focus,\n.btn-outline-destructive:focus,\n.btn-flat-destructive:focus": {
    "--btn-focus-color": "var(--error)"
  }
}