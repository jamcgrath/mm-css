{
  "button,\n.btn": {
    "backgroundColor": "var(--button-bg)",
    "color": "var(--button-color, currentcolor)",
    "border": "var(--button-border, none)",
    "borderRadius": "var(--button-border-radius, 8px)",
    "padding": "var(--button-padding, 1px 6px)",
    "fontFamily": "var(--button-font-family, var(--poppins, inherit))",
    "fontSize": "var(--button-font-size, var(--label))",
    "fontWeight": "var(--button-font-weight, var(--bold))",
    "lineHeight": "var(--button-line-height, 1.285)",
    "display": "var(--button-display, inline-block)",
    "textAlign": "var(--button-text-align, center)",
    "width": "var(--button-width, auto)",
    "height": "var(--button-height, auto)",
    "minWidth": "var(--button-min-width, auto)",
    "minHeight": "var(--button-min-height, auto)",
    "cursor": "var(--button-cursor, pointer)"
  },
  "button:hover,\n.btn:hover": {
    "backgroundColor": "var(--button-bg-hover)",
    "color": "var(--button-color-hover, var(--button-color))",
    "border": "var(--button-border-hover, currentcolor)"
  },
  "button:active,\n.btn:active": {
    "backgroundColor": "var(--button-bg-active)",
    "color": "var(--button-color-active, var(--button-color))",
    "border": "var(--button-border-active, currentcolor)"
  },
  "button:focus,\n.btn:focus": {
    "outline": "var(--focus-outline-width, 2px) var(--focus-outline-style, solid)\n    var(\n      --button-focus-outline-color,\n      var(--button-bg-hover, var(--button-color-hover))\n    )",
    "outlineOffset": "var(--button-focus-outline-offset, 2px)"
  },
  "@supports selector(:focus-visible)": {
    "button:focus,\n  .btn:focus": {
      "outline": "none",
      "outlineOffset": "0"
    },
    "button.focus-visible,\n  .btn.focus-visible": {
      "outline": "var(--focus-outline-width, 2px) var(--focus-outline-style, solid)\n      var(\n        --button-focus-outline-color,\n        var(--button-bg-hover, var(--button-color-hover))\n      )",
      "outlineOffset": "var(--button-focus-outline-offset, 2px)"
    },
    "button:focus-visible,\n  .btn:focus-visible": {
      "outline": "var(--focus-outline-width, 2px) var(--focus-outline-style, solid)\n      var(\n        --button-focus-outline-color,\n        var(--button-bg-hover, var(--button-color-hover))\n      )",
      "outlineOffset": "var(--button-focus-outline-offset, 2px)"
    }
  },
  "button[disabled],\nbutton[aria-disabled=\"true\"],\nbutton.btn-disabled,\n.btn.btn-disabled": {
    "backgroundColor": "var(--button-bg) !important",
    "color": "var(--gray-6) !important",
    "border": "var(--button-border) !important",
    "--button-bg": "var(--gray-3)",
    "--button-color": "var(--gray-6)",
    "--button-border": "none"
  },
  ".btn-sm": {
    "--button-padding": "var(--space-1) var(--space-3)"
  },
  ".btn-med": {
    "--button-padding": "var(--space-2) var(--space-4)"
  },
  ".btn-lg": {
    "--button-padding": "var(--space-3) var(--space-6)"
  },
  ".btn-icon": {
    "display": "inline-flex",
    "alignItems": "center",
    "justifyContent": "center",
    "gap": "var(--button-gap)"
  },
  ".btn-icon.btn-sm,\n.btn-icon.btn-med": {
    "--button-gap": "var(--space-1)"
  },
  ".btn-icon.btn-lg": {
    "--button-gap": "var(--space-2)"
  },
  ".btn-primary": {
    "--button-bg": "var(--grape)",
    "--button-bg-hover": "var(--grape-hover)",
    "--button-bg-active": "var(--grape-pressed)",
    "--button-color": "var(--white)",
    "--button-focus-outline-color": "var(--grape-hover)"
  },
  ".btn-primary-tonal": {
    "--button-bg": "var(--grape-20)",
    "--button-bg-hover": "var(--grape-light-hover)",
    "--button-bg-active": "var(--grape-light-pressed)",
    "--button-color": "var(--grape)",
    "--button-focus-outline-color": "var(--grape-light-hover)"
  },
  ".btn-primary-outline": {
    "--button-bg": "transparent",
    "--button-border": "1px solid var(--grape)",
    "--button-border-hover": "1px solid var(--grape-hover)",
    "--button-border-active": "1px solid var(--grape-pressed)",
    "--button-color": "var(--grape)",
    "--button-color-hover": "var(--grape-hover)",
    "--button-color-active": "var(--grape-pressed)",
    "--button-focus-outline-color": "var(--grape-hover)"
  },
  ".btn-primary-outline[disabled],\n.btn-primary-outline[aria-disabled=\"true\"],\n.btn-primary-outline.btn-disabled,\n.btn-secondary-outline[disabled],\n.btn-secondary-outline[aria-disabled=\"true\"],\n.btn-secondary-outline.btn-disabled": {
    "--button-border": "1px solid var(--gray-6)",
    "--button-bg": "transparent"
  },
  ".btn-primary-ghost": {
    "--button-bg": "transparent",
    "--button-bg-hover": "var(--dove)",
    "--button-color": "var(--grape)",
    "--button-color-hover": "var(--grape-hover)",
    "--button-color-active": "var(--grape-pressed)",
    "--button-focus-outline-color": "var(--grape-hover)"
  },
  ".btn-primary-ghost[disabled],\n.btn-primary-ghost[aria-disabled=\"true\"],\n.btn-primary-ghost.btn-disabled,\n.btn-secondary-ghost[disabled],\n.btn-secondary-ghost[aria-disabled=\"true\"],\n.btn-secondary-ghost.btn-disabled": {
    "--button-bg": "transparent"
  },
  ".btn-secondary": {
    "--button-bg": "var(--black)",
    "--button-bg-hover": "var(--gray-7)",
    "--button-bg-active": "var(--gray-8)",
    "--button-color": "var(--white)",
    "--button-focus-outline-color": "var(--gray-7)"
  },
  ".btn-secondary-tonal": {
    "--button-bg": "var(--dove)",
    "--button-bg-hover": "var(--gray-4)",
    "--button-bg-active": "var(--gray-5)",
    "--button-color": "var(--gray-7)",
    "--button-color-active": "var(--black)",
    "--button-focus-outline-color": "var(--gray-4)"
  },
  ".btn-secondary-outline": {
    "--button-bg": "transparent",
    "--button-color": "var(--black)",
    "--button-color-hover": "var(--gray-7)",
    "--button-color-active": "var(--gray-8)",
    "--button-border": "1px solid var(--black)",
    "--button-border-hover": "1px solid var(--gray-7)",
    "--button-border-active": "1px solid var(--gray-8)",
    "--button-focus-outline-color": "var(--gray-7)"
  },
  ".btn-secondary-ghost": {
    "--button-bg": "transparent",
    "--button-bg-hover": "var(--dove)",
    "--button-color": "var(--black)",
    "--button-color-active": "var(--gray-8)",
    "--button-focus-outline-color": "var(--black)"
  },
  ".btn-destructive": {
    "--button-bg": "var(--error-1)",
    "--button-bg-hover": "var(--error-hover)",
    "--button-bg-active": "var(--error-pressed)",
    "--button-color": "var(--white)",
    "--button-focus-outline-color": "var(--error-hover)"
  },
  ".btn-destructive-outline": {
    "--button-bg": "transparent",
    "--button-color": "var(--error-1)",
    "--button-color-hover": "var(--error-hover)",
    "--button-color-active": "var(--error-pressed)",
    "--button-border": "1px solid var(--error-1)",
    "--button-border-hover": "1px solid var(--error-hover)",
    "--button-border-active": "1px solid var(--error-pressed)",
    "--button-focus-outline-color": "var(--error-hover)"
  },
  ".btn-destructive-ghost": {
    "--button-bg": "transparent",
    "--button-bg-hover": "var(--dove)",
    "--button-color": "var(--error-1)",
    "--button-color-hover": "var(--error-hover)",
    "--button-color-active": "var(--error-pressed)",
    "--button-focus-outline-color": "var(--error-hover)"
  },
  ".btn-circle": {
    "--button-border-radius": "50%",
    "aspectRatio": "1/1"
  },
  ".btn-circle.btn-sm": {
    "--button-width": "var(--space-6)",
    "--button-height": "var(--space-6)"
  },
  ".btn-circle.btn-med": {
    "--button-width": "var(--space-10)",
    "--button-height": "var(--space-10)"
  },
  ".btn-circle.btn-lg": {
    "--button-width": "var(--space-14)",
    "--button-height": "var(--space-14)"
  }
}