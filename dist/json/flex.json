{
  ".flex-1": {
    "flex": "1 1 0%"
  },
  ".flex-auto": {
    "flex": "1 1 auto"
  },
  ".flex-initial": {
    "flex": "0 1 auto"
  },
  ".flex-none": {
    "flex": "none"
  },
  "@media (min-width: 1024px)": {
    ".lg-flex-1": {
      "flex": "1 1 0%"
    },
    ".lg-flex-auto": {
      "flex": "1 1 auto"
    },
    ".lg-flex-initial": {
      "flex": "0 1 auto"
    },
    ".lg-flex-none": {
      "flex": "none"
    }
  }
}