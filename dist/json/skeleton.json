{
  ".skeleton": {
    "background": "var(--skeleton)"
  },
  ".skeleton-loading,\n.skeleton-loading-children > *": {
    "background": "var(--skeleton)",
    "backgroundSize": "200% 100%",
    "animation": "skeleton-loading 1.5s linear infinite"
  },
  "@keyframes skeleton-loading": {
    "to": {
      "backgroundPositionX": "-200%"
    }
  }
}