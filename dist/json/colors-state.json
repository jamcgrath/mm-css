{
  ".navy-hover:hover": {
    "color": "var(--hover-navy)"
  },
  ".navy-light-hover:hover": {
    "color": "var(--hover-navy-light)"
  },
  ".grape:hover": {
    "color": "var(--hover-grape)"
  },
  ".grape-light-hover:hover": {
    "color": "var(--hover-grape-light)"
  },
  ".error-hover:hover": {
    "color": "var(--hover-error)"
  },
  ".navy-pressed:active": {
    "color": "var(--pressed-navy)"
  },
  ".navy-light-pressed:active": {
    "color": "var(--pressed-navy-light)"
  },
  ".grape-light-pressed:active": {
    "color": "var(--pressed-grape-light)"
  },
  ".error-pressed:active": {
    "color": "var(--pressed-error)"
  }
}